{
  "version": 2,
  "name": "gobang-game",
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "server/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "functions": {
    "server/server.js": {
      "maxDuration": 10
    }
  }
}